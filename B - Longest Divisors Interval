
import java.util.*;
public class longestDivisors{
    public static void main(String[] args){
      Scanner sc = new Scanner(System.in);
      long t = sc.nextLong();
      while(t-->0){
        long n = sc.nextLong();
        int ans;
        for(int i=1; ;i++){
            if(n%i != 0){
                ans = i-1;
                break;
            }
        }
        System.out.println(ans);
      }
    }
}
